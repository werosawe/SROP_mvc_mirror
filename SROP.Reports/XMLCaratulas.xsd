<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="XMLCaratulas" targetNamespace="http://tempuri.org/XMLCaratulas.xsd" elementFormDefault="qualified"
	xmlns="http://tempuri.org/XMLCaratulas.xsd" xmlns:mstns="http://tempuri.org/XMLCaratulas.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="Caratula">
